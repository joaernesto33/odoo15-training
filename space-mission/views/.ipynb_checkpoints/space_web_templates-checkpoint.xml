<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="mission_website">
            <t t-call="website.layout">
                <t t-set="title">Space</t>
                <div class="container">
                    <h1>Missions</h1>
                    <t t-foreach="missions" t-as="mission">
                        <h3>
                            <t t-esc="mission.mission_name"/>
                            <t t-esc="mission.fuel_amount"/>
                            <t t-esc="mission.safety_features"/>
                        </h3>
                        <p>Launch Date:
                            <span t-field="mission.launch_date"/>
                        </p>
                        
                        <p>Return Date:
                            <span t-field="mission.return_date"/>
                        </p>
                        
                        
                    </t>
                </div>
            </t>
        </template>
        
    </data>
</odoo>